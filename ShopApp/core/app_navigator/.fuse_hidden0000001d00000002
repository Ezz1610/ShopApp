import Foundation
import SwiftUI

struct RootView: View {
    @EnvironmentObject var navigator: AppNavigator

    var body: some View {
        NavigationStack {
            switch navigator.currentScreen {
            case .login:
                LoginScreen()
            case .register:
                RegisterScreen()
                
                //            case .home:
                //                HomeScreen()
            
            }
        }
    }
}
